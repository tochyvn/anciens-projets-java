	private Collection<Chambre> chambre;
	
	
	public Etat() {
		super();
	}
	
	

	/**
	 * @param libelle
	 * @param chambre
	 */
	public Etat(String libelle, Collection<Chambre> chambre) {
		super();
		this.libelle = libelle;
		this.chambre = chambre;
	}



	public void setIdEtat(int idEtat) {

	}

	

}
